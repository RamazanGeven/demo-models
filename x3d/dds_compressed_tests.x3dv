#X3D V3.2 utf8
PROFILE Interchange

PROTO Test [
  inputOutput MFString url []
  inputOutput SFVec3f translation 0 0 0
] {
  Transform {
    translation IS translation
    children Shape {
      appearance Appearance {
        texture ImageTexture { url IS url }
        textureTransform TextureTransform { scale 2 2 }
        material Material { }
      }
      geometry QuadSet {
        coord Coordinate { point [ 0 0 0, 2 0 0, 2 2 0, 0 2 0 ] }
        texCoord DEF TexCoord TextureCoordinate {
          point [ 0 0, 1 0, 1 1, 0 1, ]
        }
        solid FALSE
      }
    }
  }
}

