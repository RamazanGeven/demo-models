<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE X3D PUBLIC "ISO//Web3D//DTD X3D 3.3//EN" "http://www.web3d.org/specifications/x3d-3.3.dtd">
<X3D profile="Immersive" version="3.3" >
<head>
</head>
<Scene>

<Background skyColor="0 0 0"></Background>

<Transform translation="0 0 0">
  <Shape>
    <Text string='"blablah"'>
      <!-- Specify "justify" explicitly, because X3DOM unfortunately breaks
           the X3D specification and has different default for justify:
           https://doc.x3dom.org/author/Text/FontStyle.html
           http://www.web3d.org/documents/specifications/19775-1/V3.3/Part01/components/text.html#FontStyle
      -->
      <FontStyle DEF="MyFontStyle" justify='"BEGIN"' />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -2 0">
  <Shape>
    <Text string='&quot;blablah&quot;'>
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -4 0">
  <Shape>
    <Text string="&quot;blablah&quot;">
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -6 0">
  <Shape>
    <Text string='"You will see quotes \"around this string\""'>
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -8 0">
  <Shape>
    <Text string='&quot;You will see quotes \&quot;around this string\&quot;&quot;'>
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -10 0">
  <Shape>
    <Text string='"You will see quotes \&quot;around this string\&quot;"'>
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -12 0">
  <!-- Since you surround XML attribute with single quotes (apostrophe '),
       it doesn't really matter whether you use " or &quot; inside,
       you can mix them as you wish. -->
  <Shape>
    <Text string='&quot;You will see quotes \"around this string\&quot;"'>
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

<Transform translation="0 -14 0">
  <Shape>
    <Text string="&quot;You will see quotes \&quot;around this string\&quot;&quot;">
      <FontStyle USE="MyFontStyle" />
    </Text>
  </Shape>
</Transform>

