PerspectiveCamera {
  position -12.041908264160156 -61.640647888183594 10.612056732177734
  orientation 0.99946117401123046 -0.0232076570391655 -0.02321089617908001 1.5713337659835815
}

Background { skyColor [ 0.6 0.6 0.6 ] }

Fog {
  color 0.6 0.6 0.6
