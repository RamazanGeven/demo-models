PROFILE Interchange
COMPONENT Shaders:1

NavigationInfo {
	headlight FALSE
	visibilityLimit 100
	type ["EXAMINE", "ANY"]
	avatarSize [0.25, 1.75, 0.75]
}

Background {
	skyColor 0.5 0.5 0.5
}

DEF scr Script {
	inputOnly SFMatrix4f cameraInverseMatrix
	outputOnly SFVec3f location
	initializeOnly SFVec4f locationH 0 0 0 1
	inputOnly SFMatrix3f cameraRotationInverseMatrix
	inputOnly SFMatrix3f cameraRotationMatrix
	outputOnly SFVec3f direction
	outputOnly SFVec3f up
	outputOnly SFRotation x_rotation
	outputOnly SFRotation y_rotation
	outputOnly SFRotation z_rotation
	initializeOnly SFVec3f Y_axis 0 1 0
	url "castlescript:
		function cameraInverseMatrix (value, t)
			locationH := value * vector (0,0,0,1);
			location := vector (vector_get (locationH,0), vector_get(locationH,1), vector_get (locationH,2))
		function cameraRotationInverseMatrix (value, t)
			direction := value * vector (0,0,-1);
			up := value * vector (0,1,0);
			y_rotation := if ( vector_get (direction, 0) < 0,
				vector (0,1,0, ArcCos (
					vector_dot (
						vector (0,0,-1),
						vector (vector_get (direction, 0), 0, vector_get (direction, 2))
					) / Sqrt (Sqr (vector_get (direction, 0)) + Sqr (vector_get (direction, 2)))
				)),
				vector (0,-1,0, ArcCos (
					vector_dot (
						vector (0,0,-1),
						vector (vector_get (direction, 0), 0, vector_get (direction, 2))
					) / Sqrt (Sqr (vector_get (direction, 0)) + Sqr (vector_get (direction, 2)))
				))
			);
			x_rotation := vector (1,0,0, ArcSin (vector_get (direction, 1)))
		function cameraRotationMatrix (value, t)
			Y_axis := value * vector(0,1,0);
			z_rotation := if ( vector_get (Y_axis, 0) < 0,
				vector (0,0,-1, ArcCos (
					vector_dot (
						vector (0,1,0),
						vector (vector_get (Y_axis, 0), vector_get (Y_axis, 1), 0)
					) / Sqrt (Sqr (vector_get (Y_axis, 0)) + Sqr (vector_get (Y_axis, 1)))
				)),
				vector (0,0,1, ArcCos (
					vector_dot (
						vector (0,1,0),
						vector (vector_get (Y_axis, 0), vector_get (Y_axis, 1), 0)
					) / Sqrt (Sqr (vector_get (Y_axis, 0)) + Sqr (vector_get (Y_axis, 1)))
				))
			)
	"
}

Transform {
	scale 1 1 1
	children Inline { url "../axes.x3dv" }
}

DEF pLight PointLight {
	location 10 10 0
	radius 20
	intensity 0.75
	ambientIntensity 0.25
	color 1 1 1
	on TRUE
}

DEF Camera_Rot Transform {
	children DEF Camera_Tra Transform {
		translation 0.0 0.0 0.0
		rotation 1 0 0 0
		children [
			DEF Camera_001 Viewpoint {
				#fieldOfView 1.570796
				description "Camera.001" 
				position 0.0 2.0 5.0
				orientation 0 1 0 0
			}
		]
	}
}
ROUTE Camera_001.cameraInverseMatrix TO scr.cameraInverseMatrix
ROUTE Camera_001.cameraRotationInverseMatrix TO scr.cameraRotationInverseMatrix
ROUTE Camera_001.cameraRotationMatrix	TO scr.cameraRotationMatrix


DEF spotT1 Transform {
	translation 0 0 0
	children [
		DEF spotT2 Transform {
		rotation 0 1 0 0
		#translation 0 0 -5
		children DEF spotT3 Transform { translation 0 0 -5 children
			Shape {
				#geometry Sphere { radius 0.05 }
				geometry Box { size 1 1 1 }
				appearance Appearance {
					material Material {
						emissiveColor 1 1 1
						diffuseColor 0 0 0
						ambientIntensity 0.0
					}
					texture ImageTexture {url "../test.jpg"}
				}
			} }
		}	
	]
}
ROUTE scr.location TO spotT1.translation
ROUTE scr.y_rotation TO spotT1.rotation
ROUTE scr.x_rotation TO spotT2.rotation
ROUTE scr.z_rotation TO spotT3.rotation

DEF viewSpot SpotLight {
	location 0 0 0
	direction 0 0 -1
	radius 50
	intensity 0
	ambientIntensity 0
	color 0 0 0
	projectionNear 3
	projectionFar 50
	on FALSE
}
ROUTE scr.location TO viewSpot.location
ROUTE scr.direction TO viewSpot.direction
ROUTE scr.up TO viewSpot.up

