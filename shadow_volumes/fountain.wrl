# The camera and the non-shadow-casting lights.

PointLight {
	intensity 0.2000000029802322
	location 40 0 10
}
PointLight {
	intensity 0.2000000029802322
	location -40 0 10
}
PointLight {
	intensity 0.2000000029802322
	location 0 40 10
}
PointLight {
	intensity 0.2000000029802322
	location 0 -40 10
}

# Camera settings "encoded" in the VRML declaration below :
# direction 0.277092695236206 -0.612786591053009 -0.3090673685073852
# up 0.1720504313707351 -0.3804871141910553 0.9086408019065856
# gravityUp 0 0 1
Transform {
  translation -1.551781177520752 8.2984724044799805 9.9686040878295898
  rotation 1 0 0 1.5707963705062866
  children Viewpoint {
    position 0 0 0 # camera position is expressed by translation
    orientation -0.0460275933146476 -0.9758579730987548 -0.2135013192892074 2.7268810272216797
  }
}

# Shadowing light ------------------------------------------------------------

