#VRML V1.0 ascii

# Demo that partially transparent object works as shadow caster too.
# Assumes you have castle data in ../../castle/ directory.

PointLight {
  location 2 0 4
  kambiShadows TRUE
  kambiShadowsMain TRUE
}

Group {
  WWWInline { name "../../castle/data/creatures/ghost/ghost_header.wrl" separate FALSE }
  WWWInline { name "../../castle/data/creatures/ghost/ghost_stand_1.wrl" }
}

Translation {
  translation 0 0 -1
}

Cube {
  #width 1 height 1 depth 1
}
