#X3D V3.2 utf8
PROFILE Interchange

EXTERNPROTO BlueGeometry [
  inputOutput MFString sphereTexture
] [ "blue/prototype.x3dv#BlueGeometry" ]

EXTERNPROTO RedGeometry [
  inputOutput MFString sphereTexture
] [ "red/prototype.x3dv#RedGeometry" ]

