#VRML V1.0 ascii

# Demonstrates Fog node, I allows this in view3dscene for VRML 1.0 too.
# The fog handling is the same as in VRML 97.

Group {
  PerspectiveCamera {
    position -12.041908264160156 -61.640647888183594 10.612056732177734
    orientation 0.99946117401123046 -0.0232076570391655 -0.02321089617908001 1.5713337659835815
  }
  # bg color matching fog color
  Background { skyColor [ 0 0.5 0 ] }
  Fog {
    color 0 0.5 0
    visibilityRange 160
    fogType "EXPONENTIAL"
  }
  WWWInline { name "../castle_with_lights.wrl" }
}
