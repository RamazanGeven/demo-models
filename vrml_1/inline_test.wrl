    WWWInline { name "kings_head.wrl" }
    Scale { scaleFactor 1.2 1.2 1.2 }
    Translation { translation +3 0 0 }
    WWWInline { name "kings_head.wrl" }
  }
  Translation { translation 0 5 0 }
